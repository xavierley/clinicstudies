plugin.tx_xl_clinicstudies {
	mvc.callDefaultActionIfActionCantBeResolved = 1

	view {
		templateRootPaths {
			0 = EXT:xl_clinicstudies/Resources/Private/Templates/
			1 = {$plugin.xl_clinicstudies.view.templateRootPath}
		}

		partialRootPaths {
			0 = EXT:xl_clinicstudies/Resources/Private/Partials/
			1 = {$plugin.xl_clinicstudies.view.partialRootPath}
		}

		layoutRootPaths {
			0 = EXT:xl_clinicstudies/Resources/Private/Layouts/
			1 = {$plugin.xl_clinicstudies.view.layoutRootPath}
		}
	}
}
